{
  "project": {
    "version": "1.0.0-beta.11-webpack.2",
    "name": "webpackcli"
  },
  "apps": [
    {
      "main": "main.ts",
      "tsconfig": "tsconfig.json",
      "polyfills": "polyfills.ts",
      "mobile": false,
      "prefix": "app",
      "root": "src"
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "config/protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "config/karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "lazyRoutePrefix": "+"
  }
}
